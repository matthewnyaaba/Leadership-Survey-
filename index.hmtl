        document.getElementById('assessmentForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            // Clear previous errors
            document.querySelectorAll('.error').forEach(err => err.textContent = '');
            
            // Validate form
            const name = document.getElementById('name').value.trim();
            const email = document.getElementById('email').value.trim();
            
            if (!name) {
                document.getElementById('name-error').textContent = 'Please enter your name';
                return;
            }
            
            if (!email || !isValidEmail(email)) {
                document.getElementById('email-error').textContent = 'Please enter a valid email';
                return;
            }
            
            // Calculate total score
            let totalScore = 0;
            const answers = {};
            const totalQuestions = 33; // All questions
            
            for (let i = 1; i <= totalQuestions; i++) {
                const selectedValue = document.querySelector(`input[name="q${i}"]:checked`);
                if (selectedValue) {
                    answers[`q${i}`] = parseInt(selectedValue.value);
                    totalScore += parseInt(selectedValue.value);
                }
            }
            
            // Store response data
            const responseData = {
                timestamp: new Date().toISOString(),
                name: name,
                email: email,
                ...answers,
                totalScore: totalScore
            };
            
            responses.push(responseData);
            
            // Show success message and results
            showSuccessMessage(name);
            showResults(totalScore);
        });

